{
    "name": "Scraping Bootcamp Krowdy",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        
        "default_title": "Click Me",
        "default_popup": "popup.html"
        
        
    },
      
    
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    
    "host_permissions": [
        "https://www.linkedin.com/*" 
    ]
}